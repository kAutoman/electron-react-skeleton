{
  "name": "electron-react-skeleton",
  "description": "Electron application skeleton based on React, Webpack.",
  "version": "0.0.2",
  "main": "./main.js",
  "build": {
    "productName": "Electron-Skeleton",
    "companyName": "Electron-Skeleton",
    "appBundleId": "com.example.electron-skeleton"
  },
  "license": "MIT",
  "author": {
    "name": "Svetli Nikolov",
    "email": "me@svetli.name",
    "url": "https://github.com/svetli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svetli/electron-react-skeleton"
  },
  "keywords": [
    "electron",
    "typescript",
    "react"
  ],
  "engines": {
    "node": ">= 7",
    "yarn": ">= 1.2.0",
    "npm": ">= 4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./scripts/start",
    "start:prod": "cross-env NODE_ENV=production node ./scripts/start",
    "compile:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.development.js",
    "compile:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.production.js",
    "build:dev": "yarn compile:dev && cross-env NODE_ENV=development node ./scripts/build",
    "build:prod": "yarn compile:prod && cross-env NODE_ENV=production node ./scripts/build",
    "test": "cross-env TEST_ENV=1 xvfb-maybe electron-mocha -t 10000 --renderer --require ts-node/register app/test/*.{ts,tsx} app/test/**/*.{ts,tsx}",
    "eslint": "node ./scripts/eslint",
    "eslint:fix": "yarn eslint --fix",
    "eslint:check": "eslint --print-config .eslintrc.* | eslint-config-prettier-check",
    "tslint": "tslint ./app/{src,test}/**/*.{ts,tsx}",
    "tslint:fix": "yarn tslint --fix",
    "lint": "yarn tslint && yarn eslint:check && yarn eslint",
    "lint:fix": "yarn tslint:fix && yarn eslint:fix",
    "cleanup": "rimraf build dist node_modules"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.3",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-hot-loader": "^4.1.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.5",
    "electron-devtools-installer": "^2.2.4",
    "electron-mocha": "^6.0.4",
    "electron-packager": "^12.1.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-typescript": "^0.12.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.2.0",
    "mri": "^1.1.1",
    "node-sass": "^4.9.2",
    "prettier": "^1.13.7",
    "react-hot-loader": "^4.3.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-microsoft-contrib": "^5.1.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^17.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.3",
    "xvfb-maybe": "^0.2.1"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}